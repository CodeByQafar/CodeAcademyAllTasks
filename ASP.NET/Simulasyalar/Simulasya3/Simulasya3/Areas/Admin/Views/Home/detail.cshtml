@using Simulasya3.Models
@using Simulasya3.ViewModels.Home
@Model DetailVM



<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Team Menber @Model.FullName</h4>

            <div class="table-responsive">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Team Table</h4>
                        <div class="card" style="width: 18rem;">
                            <img class="card-img-top" src="~/assets/img/team/@Model.ImageUrl" alt="Card image cap">
                            <div class="card-body">
                                <h5 class="card-title">Id:@Model.Id ,FullName:@Model.FullName</h5>
                                <p class="card-text"> Position:@Model.Position , Order:@Model.Order</p>
                                <a asp-action="index" asp-controller="home" asp-area="admin" class="btn btn-primary">Go back</a>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>