@using Simulation6.Areas.Admin.ViewModels.Home
@model DetailVM

<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">

            <div class="card" style="width: 18rem;">
                <img src="~/assets/images/@Model.ImageUrl" class="card-img-top" alt="">

                <div class="card-body">
                    <h5 class="card-title">@Model.Title</h5>
                    <p class="card-text">@Model.ProjectType.</p>
                    <a asp-action="index" asp-controller="home" class="btn btn-primary">Back</a>
                </div>
            </div>
        </div>
    </div>
</div>